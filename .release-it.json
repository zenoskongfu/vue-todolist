{
	"git": {
		"commitMessage": "chore: release v${version}",
		"tagName": "v${version}",
		"push": true
	},
	"github": {
		"release": true
	},
	"npm": {
		"publish": true
	},
	"plugins": {
		"@release-it/conventional-changelog": {
			"preset": "angular",
			"infile": "docs/changelog/CHANGELOG.md"
		}
	}
}
